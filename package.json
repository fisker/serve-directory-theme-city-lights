{
  "name": "serve-directory-theme-city-lights",
  "version": "2.0.0",
  "description": "city lights theme for serve-directory.",
  "keywords": [
    "city-lights",
    "serve-directory",
    "serve-directory-theme",
    "theme"
  ],
  "homepage": "https://github.com/fisker/serve-directory-theme-city-lights#readme",
  "bugs": {
    "url": "https://github.com/fisker/serve-directory-theme-city-lights/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/serve-directory-theme-city-lights.git"
  },
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s build:assets build:js",
    "build:assets": "node -r esm ./scripts/build-assets.js",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:cache": "del-cli .cache",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "format:stylelint": "yarn lint:stylelint --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "lint:style": "stylelint **/*.{css,scss,less} --fix",
    "lint:stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "release": "run-s lint build format dist",
    "test": "echo \"wip\"",
    "test-coverage": "echo \"wip\""
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "pretty-bytes": "5.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@commitlint/cli": "8.3.5",
    "@fisker/commitlint-config": "1.1.6",
    "@fisker/eslint-config": "1.6.1",
    "@fisker/husky-config": "1.1.7",
    "@fisker/lint-staged-config": "1.0.10",
    "@fisker/prettier-config": "1.0.26",
    "@fisker/stylelint-config": "1.2.2",
    "@rollup/plugin-replace": "2.3.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "city-lights-icons": "github:Yummygum/city-lights-icons-atom",
    "cz-conventional-changelog": "3.1.0",
    "del-cli": "3.0.0",
    "eslint": "6.8.0",
    "esm": "3.2.25",
    "husky": "4.2.1",
    "json-stable-stringify": "1.0.1",
    "lint-staged": "10.0.7",
    "markdownlint-cli": "0.21.0",
    "mini-svg-data-uri": "1.1.3",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "rollup": "1.30.1",
    "sort-package-json": "1.39.1",
    "stylelint": "13.0.0",
    "svgo": "1.3.2",
    "write-prettier-file": "1.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "readmeFilename": "README.md"
}
