{
  "name": "serve-directory-theme-city-lights",
  "version": "1.3.0",
  "description": "city lights theme for serve-directory.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/serve-directory-theme-city-lights.git"
  },
  "keywords": [
    "serve-directory",
    "theme",
    "city-lights",
    "serve-directory-theme"
  ],
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fisker/serve-directory-theme-city-lights/issues"
  },
  "dependencies": {
    "lodash.uniq": "^4.5.0",
    "pretty-bytes": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@commitlint/cli": "8.0.0",
    "@fisker/commitlint-config": "^1.0.9",
    "@fisker/eslint-config": "^1.0.14",
    "@fisker/husky-config": "^1.1.0",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "@fisker/stylelint-config": "^1.0.8",
    "babel-plugin-add-module-exports": "1.0.2",
    "city-lights-icons": "github:Yummygum/city-lights-icons-atom",
    "cz-conventional-changelog-emoji": "0.1.0",
    "eslint": "5.16.0",
    "husky": "2.3.0",
    "json-stable-stringify": "1.0.1",
    "lint-staged": "8.1.7",
    "markdownlint-cli": "^0.16.0",
    "mini-svg-data-uri": "1.0.3",
    "node-sass": "4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.17.1",
    "stylelint": "^10.0.1",
    "svgo": "1.2.2"
  },
  "scripts": {
    "build": "cd scripts && sh build.sh",
    "release": "run-s lint build prettier dist",
    "lint": "run-p lint:*",
    "lint:style": "stylelint **/*.{css,scss,less} --fix",
    "lint:eslint": "eslint **/*.{js,mjs,vue} --fix",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "readmeFilename": "README.md",
  "files": [
    "lib"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "homepage": "https://github.com/fisker/serve-directory-theme-city-lights#readme",
  "sideEffects": false
}
